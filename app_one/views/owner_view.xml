<odoo>
   <record id="owner_view_tree" model="ir.ui.view">
        <field name="name">owner tree</field>
        <field name="model">owner</field>
        <field name="arch" type='xml'>
            <tree multi_edit='1' >
                <field name="name"/>
                <field name="phone"/>
                <field name="address"/>
            </tree>
        </field>    
   </record>
   
   <record id="owner_view_form" model="ir.ui.view">
        <field name="name">owner form</field>
        <field name="model">owner</field>
        <field name="arch" type='xml'>
            <form  >
                <sheet>
                    <div class="oe_title">
                        <label for="name"/>
                        <h1> <field name="name"/> </h1>
                    </div>
                    <group>
                            <group string="general information">
                            <field name="name"/>
                            <field name="address"/>
                            </group>
                            <group string="additional information">
                            <field name="phone"/>
                            </group>
                            <field name="property_ids" widget="many2many_tags"/>
                    </group>
                </sheet>
            </form>
        </field>    
   </record>
<record id="owner_action" model="ir.actions.act_window">
    <field name="name">Owner</field>
    <field name="res_model">owner</field>
    <!-- <field name="view_mode">tree,form</field>
    <field name="view_id" ref="app_one.owner_view_tree"/>
    <field name="view_id" ref="app_one.owner_view_form"/> -->
</record>

<menuitem
    id="owner_menu_item"
    name="Owners"
    parent='configuration_menu'
    action='owner_action'
    />



</odoo>